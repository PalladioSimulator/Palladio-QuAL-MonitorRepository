    </eAnnotations>
    <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore">
      <details key="constraints" value="retroLength"/>
    </eAnnotations>
    <eOperations name="getRetrospectionLengthAsMeasure">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="body" value=" return Measure .valueOf(getRetrospectionLength(),  javax.measure.unit.SI.SECOND);"/>
        <details key="documentation" value=" Convenience method to retrieve the property 'retrospectionLength' (interpreted as given in seconds) in terms of a javax.measure.Measure"/>
      </eAnnotations>
      <eGenericType eClassifier="ecore:EDataType platform:/plugin/org.palladiosimulator.edp2/model/EDP2.ecore#//ExperimentData/EJSMeasure">
        <eTypeArguments eClassifier="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDoubleObject"/>
        <eTypeArguments eClassifier="#//util/Duration"/>
      </eGenericType>
    </eOperations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="retrospectionLength" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble" defaultValueLiteral="10.0">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="This value (interpreted as seconds) determines the length of the retrospection interval.&#xD;&#xA;That is, it specifies which measurements shall be included in the aggregation."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"
        changeable="false" volatile="true" defaultValueLiteral="" derived="true">
      <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore/OCL">
        <details key="derivation" value="'retrospectionLength ' + self.windowLength.toString()  + ' s'"/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
